generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model UserState {
  id           String   @id @default(uuid())
  clientId     String   @unique
  completedIds String[] @default([])
  prefs        Json
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
